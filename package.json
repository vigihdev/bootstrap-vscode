{
    "name": "bootstrap-vscode",
    "description": "Bootstrap Alat devoleper Vscode",
    "author": "vigihdev@gmail.com",
    "publisher": "vigihdev",
    "version": "1.0.0",
    "license": "MIT",
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:javascript",
        "onLanguage:php",
        "onLanguage:html",
        "onLanguage:css",
        "onLanguage:scss"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/vigihdev/bootstrap-vscode.git"
    },
    "contributes": {
        "commands": [
            {
                "command": "Bootstrap.Yii2.Update",
                "title": "Bootstrap Yii2 Update Class From Assets",
                "category": "Bootstrap Yii2 Update"
            }
        ]
    },
    "main": "out/extension.js",
    "files": [
        "out/**/*"
    ],
    "scripts": {
        "del:out": "rm -r out",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "lint": "eslint src --ext ts --fix"
    },
    "devDependencies": {
        "@types/node": "^16.11.7",
        "@types/vscode": "^1.32.0",
        "@typescript-eslint/eslint-plugin": "^5.49.0",
        "@typescript-eslint/parser": "^5.49.0",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "@types/express": "^4.17.15",
        "express": "^4.18.2",
        "promise-socket": "^7.0.0"
    }
}